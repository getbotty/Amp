{
    "name":             "amp",
    "version":          "1.0.0",
    "description":      "Better form controls for complicated web applications",
    "author":           "Dusko Jordanovski <jordanovski.d@quipugmbh.com>",

    "dependencies": {
        "jquery":       ">=1.8.2",
        "underscore":   ">=1.3"
    },

    "builds": {
        "extensions":   [ "./extensions" ],
        "barebones":    [ "./extensions", "./core", "./button", "./input", "./list", "./select", "./datepicker", "./modal", "./tooltip" ],
        "grid":         [ "barebones", "./grid" ],
        "classic":      [ "barebones", "./theme-classic" ]
       
    }
}